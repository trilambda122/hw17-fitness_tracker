{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars": "warn",
        "indent": ["error", 2],
        "prefer-destructuring":"warn",
        "eol-last":"off",
        "func-names":"off",
        "space-before-function-paren":"off",
        "arrow-parens":"off"
    }
}
